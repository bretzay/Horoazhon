<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="Horoazhon.Features.Shell.Views.ShellWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Horoazhon.Features.Shell.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewsMedecins="using:Horoazhon.Features.Personnes.Views"
    xmlns:viewsClients="using:Horoazhon.Features.Clients.Views"
    xmlns:viewsConsult="using:Horoazhon.Features.Visite.Views"
    mc:Ignorable="d"
    Title="ShellWindow">


    <Grid>
        <NavigationView SelectionChanged="NavigationView_SelectionChanged">
            <NavigationView.MenuItems>
                <NavigationViewItem Content="Tableau de bord" Icon="Manage" ToolTipService.ToolTip="Home" Tag="Tableau"/>
                <NavigationViewItem Content="Agents" Icon="OtherUser" ToolTipService.ToolTip="Agents" Tag="Agents"/>
                <NavigationViewItem Content="Clients" Icon="People" ToolTipService.ToolTip="Clients" Tag="Clients"/>
                <NavigationViewItem Content="Prorpriétés" Icon="People" ToolTipService.ToolTip="Prorpriétés" Tag="Prorpriétés"/>
            </NavigationView.MenuItems>
            <Frame x:Name="Root" />
        </NavigationView>

    </Grid>
</Window>
